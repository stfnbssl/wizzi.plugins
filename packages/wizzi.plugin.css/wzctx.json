{
    "name": "wizzi.plugin.css",
    "npmName": "@wizzi/plugin.css",
    "pluginBasename": "",
    "version": "0.8.2",
    "description": "Css language",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "name": "MIT",
        "copy": "copy 2024"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        },
        "useCssParser": true,
        "usePostCss": true
    },
    "Schemas": [
        {
            "name": "css",
            "title": "css wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "hasCheatsheetFolder": true,
            "modelTransformers": [],
            "artifactGenerators": [
                {
                    "name": "document",
                    "outmime": "css",
                    "contentType": "text/css",
                    "isDefault": true
                }
            ],
            "defaultArtifact": "document",
            "wizzifiers": [
                {
                    "name": "css"
                }
            ],
            "fileExtensions": [
                "css"
            ],
            "includes": [
                {
                    "Name": "Svg",
                    "name": "svg",
                    "options": []
                }
            ]
        }
    ]
}
