module
    kind es6
    $
        var md = {
            plugin: 'wizzi.plugin.text',
            name: 'extended',
            schema: 'text',
            rootElement: '',
            checkSchema: true
        };
    trans( &md )
        $append main
            set transformedModel
                {
            foreach item in model.items
                _ doitem
                    @ item
                    @ transformedModel
            _ callback
                @ null
                @ transformedModel
    function doitem
        param parent
        param resultObj
        var f = functors[parent.wzElement]
        if f
            _ f
                @ parent
                @ resultObj
    var functors = {}
