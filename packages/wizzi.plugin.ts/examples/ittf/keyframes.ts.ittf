module

    keyframes breath-animation
        keyframe 0%
            height 100px
            width 100px
        keyframe 30%
            height 400px
            width 400px
            opacity 1
        keyframe 40%
            height 405px
            width 405px
            opacity 0.3
        keyframe 100%
            height 100px
            width 100px
            opacity 0.6

