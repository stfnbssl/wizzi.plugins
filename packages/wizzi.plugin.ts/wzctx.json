{
    "name": "wizzi.plugin.ts",
    "npnName": "@wizzi/plugin.ts",
    "pluginBasename": "ts",
    "version": "0.8.3",
    "description": "wizzi.plugin.ts",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "name": "MIT",
        "copy": "copy 2023"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        },
        "useBabel": true,
        "babel": {
            "useParser": true,
            "useParserTypescript": true
        },
        "useBabelParser": true,
        "useBabelParserTypescript": true
    },
    "Schemas": [
        {
            "name": "ts",
            "title": "ts wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "includes": [],
            "modelTransformers": [],
            "artifactGenerators": [
                {
                    "name": "module",
                    "outmime": "ts",
                    "contentType": "text/typescript",
                    "isDefault": true
                }
            ],
            "defaultArtifact": "module",
            "wizzifiers": [
                {
                    "name": "ts"
                }
            ],
            "fileExtensions": [
                "ts",
                "tsx"
            ]
        }
    ]
}
