{
    "name": "wizzi.plugin.ts",
    "npnName": "@wizzi/plugin.ts",
    "pluginBasename": "ts",
    "version": "0.8.2",
    "description": "wizzi.plugin.ts",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "name": "MIT",
        "copy": "copy 2023"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        },
        "useBabelParser": true,
        "useBabelParserTypescript": true
    },
    "Schemas": [
        {
            "name": "ts",
            "title": "ts wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "modelTransformers": [
                {
                    "name": "extended"
                }
            ],
            "artifactGenerators": [
                {
                    "name": "document"
                }
            ],
            "artifacts": [
                {
                    "name": "module",
                    "outmime": "ts",
                    "isDefault": true
                }
            ],
            "defaultArtifact": "module",
            "wizzifiers": [
                {
                    "name": "ts"
                }
            ],
            "fileExtensions": [
                "ts"
            ],
            "includes": []
        }
    ]
}
