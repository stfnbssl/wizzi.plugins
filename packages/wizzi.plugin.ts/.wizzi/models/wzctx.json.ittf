{
    $include versions
    name "wizzi.plugin.ts"
    npnName "@wizzi/plugin.ts"
    pluginBasename ""
    version '${versions['@wizzi/plugin.ts@next']}'
    description "Wizzi plugin for typescript documents"
    { author
        name "Stefano Bassoli"
        email "stfn.bssl@gmail.com"
    { github
        userid "stfnbssl"
    { license
        name ""
        copy "copy 2023"
    { Build
        isWizziPlugin true
        useWizziV08 true
        { wizziV08
        { utils
            useStringifySafe true
            useObjectInspect true
        useBabelParser true
        useBabelParserTypescript true
    [ Schemas
        {
            name "ts"
            title "ts wizzi schema"
            rootElement "xmodule"
            rootTag "module"
            builtin false
            hasWizzifier true
            [ modelTransformers
            [ artifactGenerators
                {
                    name "module"
                    outmime "ts"
                    contentType "text/typescript"
                    isDefault true
            defaultArtifact "module"
            [ wizzifiers
                {
                    name "ts"
            [ fileExtensions
                "ts"
                "tsx"
            [ includes
