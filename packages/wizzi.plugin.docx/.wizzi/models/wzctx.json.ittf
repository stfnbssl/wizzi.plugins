{
    $include versions
    name "wizzi.plugin.docx"
    npmName "@wizzi/plugin.docx"
    pluginBasename ""
    version '${versions['@wizzi/plugin.docx@next']}'
    description "Docx document"
    main "index.js"
    { author
        name "Stefano Bassoli"
        email "stfn.bssl@gmail.com"
    { github
        userid "stfnbssl"
    { license
        type "MIT"
        url "https://github.com/stfnbssl/wizzi.plugins/tree/main/packages/wizzi.plugin.docx/license.text"
        copy "copy 2024"
    { Build
        isWizziPlugin true
        useWizziV08 true
        { wizziV08
        { utils
            useStringifySafe true
            useObjectInspect true
        useDocx true
    [ Schemas
        {
            name "docx"
            title "docx wizzi schema"
            builtin false
            [ modelTransformers
            [ artifactGenerators
                {
                    name "document"
                    outmime "docx"
                    contentType "application/vnd.openxmlformats-officedocument.wordprocessingml.document"
                    isDefault true
            defaultArtifact "document"
            [ wizzifiers
            [ fileExtensions
                "docx"
            [ includes
