wfschema md
    
    preserve-tags true
    mtree-is-preprocessed true
    unknown-element-replacer text
    
    e node
        r node/s
    
    e md : node
        is-root
    
    e htmlInclude : element
        tag ::html
        include html html
    e cssInclude : element
        tag ::css
        include css css
    e jsonInclude : element
        tag ::json
        include json {
    
