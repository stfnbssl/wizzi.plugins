wfschema md
    
    preserve-tags true
    mtree-is-preprocessed true
    unknown-element-replacer text
    
    e node
        r node/s
    
    e md : node
        is-root
    
    e htmlInclude : node
        tag ::html
        include html html
    e svgInclude : element
        tag ::svg
        include svg svg
    
