    "license": {
        "name": "MIT",
        "copy": "copy 2022"
    },
    "Build": {
        "isWizziPlugin": true,
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        }
    },
    "Schemas": [
        {
            "name": "md",
            "title": "md wizzi schema",
            "builtin": false,
            "modelTransformers": [
                {
                    "name": "extended"
                }
            ],
            "artifactGenerators": [
                {
                    "name": "document"
                }
            ],
            "includes": [
                {
                    "Name": "Html",
                    "name": "html",
                    "options": []
                },
                {
                    "Name": "Svg",
                    "name": "svg",
                    "options": []
                }
            ]
        }
    ]
}
