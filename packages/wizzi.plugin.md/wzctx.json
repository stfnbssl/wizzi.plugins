        "copy": "copy 2023"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        },
        "useMarkdownParser": true,
        "useDomPurify": true
    },
    "Schemas": [
        {
            "name": "md",
            "title": "md wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "modelTransformers": [
                {
                    "name": "extended"
                }
            ],
            "artifactGenerators": [
                {
                    "name": "document"
                }
            ],
            "wizzifiers": [
                {
                    "name": "md"
                }
            ],
            "includes": [
                {
                    "Name": "Html",
                    "name": "html",
                    "options": []
                },
                {
                    "Name": "Css",
                    "name": "css",
                    "options": []
                }
            ]
        }
    ]
