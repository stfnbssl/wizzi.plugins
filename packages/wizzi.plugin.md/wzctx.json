{
    "name": "wizzi.plugin.md",
    "npnName": "@wizzi/plugin.md",
    "pluginBasename": "",
    "version": "0.8.1",
    "description": "Wizzi plugin for ",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "name": "",
        "copy": "copy 2023"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        },
        "useMarkdownParser": true,
        "useDomPurify": true
    },
    "Schemas": [
        {
            "name": "md",
            "title": "md wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "modelTransformers": [],
            "artifactGenerators": [
                {
                    "name": "document",
                    "outmime": "md",
                    "contentType": "text/html",
                    "isDefault": true
                },
                {
                    "name": "tocss",
                    "outmime": "css",
                    "contentType": "text/css",
                    "isDefault": false
                },
                {
                    "name": "tohtml",
                    "outmime": "html",
                    "contentType": "text/html",
                    "isDefault": false
                }
            ],
            "defaultArtifact": "document",
            "wizzifiers": [
                {
                    "name": "md"
                }
            ],
            "fileExtensions": [
                "md"
            ],
            "includes": [
                {
                    "Name": "Html",
                    "name": "html",
                    "options": []
                },
                {
                    "Name": "Css",
                    "name": "css",
                    "options": []
                },
                {
                    "Name": "Json",
                    "name": "json",
                    "options": []
                }
            ]
        }
    ]
}
