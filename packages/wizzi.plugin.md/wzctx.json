{
    "name": "wizzi.plugin.md",
    "version": "0.0.1",
    "description": "wizzi.plugin.md",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
