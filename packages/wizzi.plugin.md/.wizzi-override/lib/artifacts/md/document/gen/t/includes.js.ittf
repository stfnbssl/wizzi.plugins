$group
    async-md( htmlInclude )
        if model.get_html
            _ included_writers.writeIncludeHtml
                @ ctx
                @ model
                a_cb( notUsed )
                    r_cb( true )
        else
            r_cb( true )
    async-md( cssInclude )
        # do nothing, css must be produced by the 'tocss' artifact generator
        r_cb( true )
