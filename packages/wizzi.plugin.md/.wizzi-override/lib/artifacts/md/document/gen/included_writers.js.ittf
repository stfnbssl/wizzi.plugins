module
    kind es6
    +
    var myname = 'md.document.included_writers'
    +
    var verify = require('wizzi-utils').verify
    +
    var md = module.exports = {}
    set md.writeIncludeHtml
        function
            param ctx
            param element
            param callback
            # log myname, 'enter writeIncludeHtml'
            _ element.get_html
                a_cb( htmlModel )
                    # log myname, 'htmlModel', htmlModel
                    _ ctx.wizziFactory.generateArtifact
                        @ htmlModel
                        @ 'generated from md model'
                        @ 'html/document'
                        {
							@ noGeneratorComments true
                        a_cb( artifactText )
                            # log myname, 'html module artifactText', artifactText
                            _ ctx.writeAligned(artifactText)
                            return
                                _ callback
