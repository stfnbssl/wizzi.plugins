module
    kind es6
    $
        var md = {
            plugin: 'wizzi.plugin.json',
            name: 'toml',
            schema: 'json',
            rootElement: '',
            checkSchema: false,
            modelIsObjectOrArray: true,
            isAsync: true,
        };
        
    gen( &md )
        $append imports
        $append main
            # loog 'model', model
            delete model.___exportName
            foreach key in Object.keys(model)
                var item = model[key]
                # loog 'key', key
                if verify.isObject(item)
                    _ ctx.w
                        @ '[' + key + ']'
                    _ writeObject
                        @ item
                        @ ctx
                        @ true
                elif verify.isArray(item)
                    _ ctx.w
                        @ key + ' = [' + writeArray(item, ctx) + ']'
                else
                    # loog 'key', key, 'value', verify.isString(item) ? '"' + item + '"' : item
                    _ ctx.w
                        @ key + ' = ' + (verify.isString(item) ? ('"' + item + '"') : item)
            r_cb(ctx)

    function writeArray
        param arr
        param ctx
        param sb
        var sb = sb || []
        var first = true
        foreach item in arr
            if !first
                _ sb.push(', ')
            if verify.isObject(item)
                _ sb.push
                    @ '{' + writeObject(item, ctx, sb) + '}' + '\n'
            elif verify.isArray(item)
                _ sb.push
                    @ '[' + writeArray(item, ctx, sb) + ']' + '\n'
            else
                _ sb.push
                    @ verify.isString(item) ? '"' + item + '"' : item + '\n'
            set first = false
        return sb.join('') 

    function writeObject
        param obj
        param ctx
        param top
        param sb
        var sb = sb || []
        if top
            foreach key in Object.keys(obj)
                var item = obj[key]
                if verify.isObject(item)
                    _ sb.push
                        @ key + ' = ' + '\n'
                    _ writeObject
                        @ item
                        @ ctx
                        @ false
                        @ sb
                elif verify.isArray(item)
                    _ sb.push
                        @ key + ' = [' + writeArray(item, ctx, sb) + ']' + '\n'
                else
                    _ sb.push
                        @ key + ' = ' + (verify.isString(item) ? ('"' + item + '"') : item)
        return sb.join('') 

