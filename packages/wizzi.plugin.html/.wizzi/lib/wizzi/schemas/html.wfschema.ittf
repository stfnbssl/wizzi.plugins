wfschema html
    
    preserve-tags true
    mtree-is-preprocessed true
    unknown-element-replacer text
    
    e node
        r node/s
    
    e html : node
        is-root
    
    e jsInclude : node
        tag ::script
        include js module
    e cssInclude : node
        tag ::style
        include css css
    e svgInclude : node
        tag ::svg
        include svg svg
    e vttInclude : node
        tag ::vtt
        include vtt vtt
    

