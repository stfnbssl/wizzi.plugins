    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        }
    },
    "Schemas": [
        {
            "name": "html",
            "title": "html wizzi schema",
            "builtin": false,
            "hasWizzifier": true,
            "modelTransformers": [],
