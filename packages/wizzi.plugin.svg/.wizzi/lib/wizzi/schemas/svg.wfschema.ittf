wfschema svg
    
    preserve-tags true
    mtree-is-preprocessed true
    unknown-element-replacer text
    
    e node
        r node/s
    
    e svg : node
        is-root
    
    e jsInclude : element
        tag ::script
        include js module
    
    e rect : node
        a x
        a y
        a width
        a height
