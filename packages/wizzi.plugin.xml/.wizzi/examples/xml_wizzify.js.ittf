module
    kind es6
    $
        var md = {
            name: 'xml',
        };
    wizziplugin/examples/plugin( &md )
        $append plugins
            [ items
                @ './wizzi.plugin.xml/index.js'
            @ pluginsBaseFolder
                _ path.resolve
                    @ __dirname
                    @ '..'
                    @ '..'
        $append imports
            var xmlwizzifier = require('../lib/wizzifiers/xml/wizzifier')
        +
        let arg = process.argv[2]
        const moduleName = arg && arg.length > 0 ? arg : 'first'
        _ async.map
            [
                @ moduleName
            @ wizzify
            a_cb_no_throw( result )
                info ''
                info 'Terminated. result: ', result
        function wizzify
            param name
            param callback
            +
            var source
                _ path.join
                    @ __dirname
                    @ 'data'
                    @ name + '.xml'
            +
            _ createWizziFactory
                {
                a_cb_no_throw( wf )
                    _ xmlwizzifier.getWizziIttf
                        _ file.read
                            @ source
                        {
                            @ wf wf
                            @ syntaxOutFile
                                _ path.join
                                    @ __dirname
                                    @ 'data'
                                    @ 'output'
                                    @ name + '.xml.syntax'
                        a_cb_no_throw( ittf )
                            if err
                                error 'error wizzifying: ' + source
                                error 'err', err
                                return callback(null, 'error ' + source)
                            _ file.write
                                _ path.join
                                    @ __dirname
                                    @ 'data'
                                    @ 'output'
                                    @ name + '.xml.ittf'
                                @ ittf
                            return callback(null, 'success ' + source)
