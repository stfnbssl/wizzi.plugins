    # Stephen Cronin <https://github.com/cronin4392>
    # TypeScript Version: 3.x
    :export-ns PptxGenJS
    export-default PptxGenJS
    :declare 
        class PptxGenJS
            #
                # *
                # * PptxGenJS Library Version
                # * @type {string}
                # 
            p version
                :readonly 
                :string 
            # Exposed prop types
            p presLayout
                :readonly 
                :ref PptxGenJS.PresentationProps
            p AlignH
                :readonly 
                :typeof 
                    :qname PptxGenJS.AlignH
            p AlignV
                :readonly 
                :typeof 
                    :qname PptxGenJS.AlignV
            p ChartType
                :readonly 
                :typeof 
                    :qname PptxGenJS.ChartType
            p OutputType
                :readonly 
                :typeof 
                    :qname PptxGenJS.OutputType
            p SchemeColor
                :readonly 
                :typeof 
                    :qname PptxGenJS.SchemeColor
            p ShapeType
                :readonly 
                :typeof 
                    :qname PptxGenJS.ShapeType
            # Presentation Props
            #
                # *
                # * Presentation layout name.
                # * Standard layouts:
                # * - 'LAYOUT_4x3'   (10" x 7.5")
                # * - 'LAYOUT_16x9'  (10" x 5.625")
                # * - 'LAYOUT_16x10' (10" x 6.25")
                # * - 'LAYOUT_WIDE'  (13.33" x 7.5")
                # *
                # * Custom layouts:
                # * - Use `pptx.defineLayout()` to create custom layouts (e.g.: 'A4')
                # *
                # * @type {string}
                # * @see https://support.office.com/en-us/article/Change-the-size-of-your-slides-040a811c-be43-40b9-8d04-0de5ed79987e
                # 
            p layout
                :string 
            #
                # *
                # * Whether Right-to-Left (RTL) mode is enabled
                # * @type {boolean}
                # 
            p rtlMode
                :boolean 
            # Presentation Metadata
            #
                # *
                # * Author name
                # * @type {string}
                # 
            p author
                :string 
            #
                # *
                # * Comapny name
                # * @type {string}
                # 
            p company
                :string 
            #
                # *
                # * @type {string}
                # * @note the `revision` value must be a whole number only (without "." or "," - otherwise, PowerPoint will throw errors upon opening!)
                # 
            p revision
                :string 
            #
                # *
                # * Presentation subject
                # * @type {string}
                # 
            p subject
                :string 
            #
                # *
                # * Presentation name
                # * @type {string}
                # 
            p title
                :string 
            # Methods
            #
                # *
                # * Export the current Presentation to stream
                # * @returns {Promise<string | ArrayBuffer | Blob | Uint8Array>} file stream
                # 
            :m stream
                :return
                    :ref Promise
                        :param 
                            :union 
                                :string 
                                :ref ArrayBuffer
                                :ref Blob
                                :ref Uint8Array
            #
                # *
                # * Export the current Presentation as JSZip content with the selected type
                # * @param {JSZIP_OUTPUT_TYPE} outputType - 'arraybuffer' | 'base64' | 'binarystring' | 'blob' | 'nodebuffer' | 'uint8array'
                # * @returns {Promise<string | ArrayBuffer | Blob | Uint8Array>} file content in selected type
                # 
            :m write
                param props
                    :ref PptxGenJS.WriteProps
                    :optional 
                :return
                    :ref Promise
                        :param 
                            :union 
                                :string 
                                :ref ArrayBuffer
                                :ref Blob
                                :ref Uint8Array
            #
                # *
                # * Export the current Presentation. Writes file to local file system if `fs` exists, otherwise, initiates download in browsers
                # * @param {string} exportName - file name
                # * @returns {Promise<string>} the presentation name
                # 
            :m writeFile
                param props
                    :ref PptxGenJS.WriteFileProps
                    :optional 
                :return
                    :ref Promise
                        :param string
            #
                # *
                # * Add a new Section to Presentation
                # * @param {SectionProps} section - section properties
                # * @example pptx.addSection({ title:'Charts' });
                # 
            :m addSection
                param section
                    :ref PptxGenJS.SectionProps
                :return
                    :void 
            #
                # *
                # * Add a new Slide to Presentation
                # * @param {AddSlideProps} options - slide options
                # * @returns {Slide} the new Slide
                # 
            :m addSlide
                param options
                    :ref PptxGenJS.AddSlideProps
                    :optional 
                :return
                    :ref PptxGenJS.Slide
            #
                # *
                # * Add a new Slide to Presentation
                # * @param {string} masterName - master slide name
                # * @returns {Slide} the new Slide
                # * @deprecated use `addSlide(IAddSlideOptions)`
                # 
            :m addSlide
                param masterName
                    :string 
                    :optional 
                :return
                    :ref PptxGenJS.Slide
            #
                # *
                # * Create a custom Slide Layout in any size
                # * @param {PresLayout} layout - an object with user-defined w/h
                # * @example pptx.defineLayout({ name:'A3', width:16.5, height:11.7 });
                # 
            :m defineLayout
                param layout
                    :ref PptxGenJS.PresLayout
                :return
                    :void 
            #
                # *
                # * Create a new slide master [layout] for the Presentation
                # * @param {SlideMasterProps} slideMasterOpts - layout definition
                # 
            :m defineSlideMaster
                param props
                    :ref PptxGenJS.SlideMasterProps
                :return
                    :void 
            #
                # *
                # * Reproduces an HTML table as a PowerPoint table - including column widths, style, etc. - creates 1 or more slides as needed
                # * @param {string} eleId - table HTML element ID
                # * @param {TableToSlidesProps} options - generation options
                # 
            :m tableToSlides
                param eleId
                    :string 
                param options
                    :ref PptxGenJS.TableToSlidesProps
                    :optional 
                :return
                    :void 
