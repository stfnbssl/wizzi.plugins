svg
    view-box 0 0 800 120
    xmlns http://www.w3.org/2000/svg
    defs
        linear-gradient
            id barGradient
            x1 0%
            y1 0%
            x2 100%
            y2 0%
            stop
                offset 0%
                style stop-color:#2d9cdb;stop-opacity:1
            stop
                offset 50%
                style stop-color:#56ccf2;stop-opacity:1
            stop
                offset 100%
                style stop-color:#2d9cdb;stop-opacity:1
        linear-gradient
            id barGradient2
            x1 0%
            y1 0%
            x2 100%
            y2 0%
            stop
                offset 0%
                style stop-color:#4a90a4;stop-opacity:1
            stop
                offset 50%
                style stop-color:#7fb3d3;stop-opacity:1
            stop
                offset 100%
                style stop-color:#4a90a4;stop-opacity:1
    $if false
        line
            x1 140
            y1 10
            x2 140
            y2 110
            stroke #e0e0e0
            stroke-width 2
        line
            x1 260
            y1 10
            x2 260
            y2 110
            stroke #e0e0e0
            stroke-width 2
        line
            x1 380
