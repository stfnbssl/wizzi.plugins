            stroke-width 2
    $if false
        rect
            x 30
            y 45
            width 100
            height 30
            $$ rx 15
            $$ ry 15
            fill url(#barGradient)
    $if false
        path
            d M 10,10H 150A 40,40 0 0 1 150,90H 10Z
            stroke #333
            stroke-width 1        
            fill url(#barGradient)
    $if false
        path
            d M 10,10A 40,40 0 0 1 10,90H 150V10 Z
            stroke #333
            stroke-width 1        
            fill url(#barGradient)
    $if false
        path
            d M 10,10A 40,40 0 0 1 10,90H 150A 40,40 0 1 0 150,10Z
            stroke #333
            stroke-width 1        
            fill url(#barGradient)
    $
        var opt = {};
        opt.left = 60;
        opt.top = 20;
        opt.right = 260;
        opt.bottom = 60;
        opt.flagHeight = 200;
        opt.lines = ['aaa','bbb','ccc']
        //
