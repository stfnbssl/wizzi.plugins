            ],
            "defaultArtifact": "document",
            "wizzifiers": [
                {
                    "name": "bin"
                }
            ],
            "fileExtensions": [
                "bin",
                "bmp",
                "gif",
                "ico",
                "jpg",
                "jpeg",
                "otf",
                "png",
                "tif",
                "tiff",
                "ttf",
                "woff"
