    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        }
    },
    "Schemas": [
        {
            "name": "bin",
            "title": "bin wizzi schema",
            "builtin": true,
            "hasWizzifier": true,
