plain
    contents module\n    kind jsfile\n    var verify = require('@wizzi/utils').verify\n    function cleanAst\n        param ast\n        delete ast.loc\n        foreach k in Object.keys(ast)\n            if verify.isArray(ast[k])\n                # loog 'k', k\n                var temp = []\n                foreach node in ast[k]\n                    if node.type === 'space'\n                    else\n                        _ cleanAst(node)\n                        _ temp.push(node)\n                set ast[k] = temp\n            if verify.isObject(ast[k])\n                # loog 'k', k\n                _ cleanAst(ast[k])\n        return ast\n    set module.exports = cleanAst\n
