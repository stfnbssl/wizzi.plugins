module
    kind jsfile
    $
        var md = {
            name: 'plain',
        };
    +
    +
    import async
    import path
    import util
    +
    var plainwizzifier = require('../lib/wizzifiers/plain/wizzifier')
    var file = require('@wizzi/utils').file
    +
    let arg = process.argv[2]
    const moduleName = arg && arg.length > 0 ? arg : 'first'
    _ async.map
        [
            @ moduleName
        @ wizzify
        a_cb_no_throw( result )
            info ''
            info 'Terminated. result: ', result
    function wizzify
        param name
        param callback
        +
        var source
            _ path.join
                @ __dirname
                @ 'data'
                @ name + '.plain'
        +
        _ plainwizzifier.getWizziIttf
            _ file.read
                @ source
            {
                @ syntaxOutFile
                    _ path.join
                        @ __dirname
                        @ 'data'
                        @ 'output'
                        @ name + '.plain.syntax'
            a_cb_no_throw( ittf )
                if err
                    error 'error wizzifying: ' + source
                    error 'err', err
                    return callback(null, 'error ' + source)
                _ file.write
                    _ path.join
                        @ __dirname
                        @ 'data'
                        @ 'output'
                        @ name + '.plain.ittf'
                    @ ittf
                return callback(null, 'success ' + source)
