module
    kind es6
    $
        var md = {
            name: 'wfschema',
        };
    v08/examples/plugin( &md )
        $append plugins
            [ items
                @ './index.js'
                @ 'wizzi-js'
            @ pluginsBaseFolder
                _ path.resolve
                    @ __dirname
                    @ '..'
        let arg = process.argv[2]
        const moduleName = arg && arg.length > 0 ? arg : 'first'
        _ executeGenerateModules
            [
                @ moduleName
            a_cb_no_throw( result )
                if err
                    error 'wfschema.examples.executeGenerateModules.err', err
                    error 'wfschema.examples.executeGenerateModules.err.toString()', err.toString()
                    if err.inner
                        error 'wfschema.examples.executeGenerateModules.err.inner.toString()', err.inner.toString()
                else
        function executeGenerateModules
            param modules
            param callback
            _ async.mapSeries
                @ modules
                =>
                    param module
                    param callback
                    log 'wfschema.example.executeGenerateModules.module: ' + module
                    var ittfDocumentUri
                        _ path.join
                            @ __dirname
                            @ 'ittf'
                            @ module + '.wfschema.ittf'
                    var outputLoadedPath
                        _ path.join
                            @ __dirname
                            @ 'results'
                            @ module + '.wfschema.json'
                    var outputExtendedPath
                        _ path.join
                            @ __dirname
                            @ 'results'
                            @ module + '.wfschema.json_docs.json'
                    _ loadWizziModel
                        @ ittfDocumentUri
                        {
                        a_cb( model )
                            log 'wfschema.example.executeGenerateModules.outputLoadedPath: ' + outputLoadedPath
                            _ file.write
                                @ outputLoadedPath
                                _ stringify
                                    @ model.toJson()
                                    @ null
                                    @ 4
                            _ loadModelAndTransform
                                @ ittfDocumentUri
                                {
                                @ "wfschema/json_docs"
                                a_cb( model )
                                    log 'wfschema.example.executeGenerateModules.outputExtendedPath: ' + outputExtendedPath
                                    _ file.write
                                        @ outputExtendedPath
                                        _ stringify
                                            @ model
                                            @ null
                                            @ 4
                                    r_cb( )
                @ callback
