{
    "name": "wizzi.plugin.wzschema",
    "npmName": "@wizzi/plugin.wzschema",
    "pluginBasename": "",
    "version": "0.8.3",
    "description": "Wizzi Schema definition",
    "author": {
        "name": "Stefano Bassoli",
        "email": "stfn.bssl@gmail.com"
    },
    "github": {
        "userid": "stfnbssl"
    },
    "license": {
        "name": "MIT",
        "copy": "copy 2024"
    },
    "Build": {
        "isWizziPlugin": true,
        "useWizziV08": true,
        "wizziV08": {},
        "utils": {
            "useStringifySafe": true,
            "useObjectInspect": true
        }
    },
    "Schemas": [
        {
            "name": "wzschema",
            "title": "wzschema wizzi schema",
            "builtin": false,
            "modelTransformers": [
                {
                    "name": "json_docs"
                },
                {
                    "name": "schemaboot"
                }
            ],
            "artifactGenerators": [
                {
                    "name": "model",
                    "outmime": "js",
                    "contentType": "text/javascript",
                    "isDefault": true
                },
                {
                    "name": "html_docs",
                    "outmime": "html",
                    "contentType": "text/html",
                    "isDefault": false
                },
                {
                    "name": "factory",
                    "outmime": "js",
                    "contentType": "text/javascript",
                    "isDefault": false
                },
                {
                    "name": "text",
                    "outmime": "js",
                    "contentType": "text/javascript",
                    "isDefault": false
                },
                {
                    "name": "json_schemaboot",
                    "outmime": "json",
                    "contentType": "application/json",
                    "isDefault": false
                }
            ],
            "defaultArtifact": "model",
            "wizzifiers": [],
            "fileExtensions": [
                "wzschema"
            ],
            "includes": [
                {
                    "Name": "Js",
                    "name": "js",
                    "options": []
                },
                {
                    "Name": "Html",
                    "name": "html",
                    "options": []
                }
            ]
        }
    ]
}
