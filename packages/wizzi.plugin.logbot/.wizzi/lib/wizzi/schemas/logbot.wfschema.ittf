wfschema logbot
    
    preserve-tags true
    mtree-is-preprocessed true
    unknown-element-replacer text
    
    e node
        r node/s
    
    e logbot : node
        is-root
    
    e jsInclude : node
        tag ::js
        include js module
    e jsonInclude : node
        tag ::json
        include json {
    